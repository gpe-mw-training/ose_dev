{
    "kind": "List",
    "apiVersion": "v1beta3",
    "metadata": {},
    "items": [
        {
            "apiVersion": "v1",
            "kind": "Service",
            "metadata": {
                "annotations": {
                    "description": "The web server's http port."
                },
                "labels": {
                    "application": "session-replication",
                    "template": "eap6-basic-sti"
                },
                "name": "session-replication"
            },
            "spec": {
                "ports": [
                    {
                        "port": 8080,
                        "targetPort": 8080
                    }
                ],
                "selector": {
                    "deploymentConfig": "session-replication"
                }
            }
        },
        {
            "apiVersion": "v1",
            "kind": "Service",
            "metadata": {
                "annotations": {
                    "description": "Ping service for clustered applications."
                },
                "labels": {
                    "application": "session-replication",
                    "template": "eap6-basic-sti"
                },
                "name": "session-replication-ping"
            },
            "spec": {
                "portalIP": "None",
                "ports": [
                    {
                        "port": 8888,
                        "targetPort": 8888
                    }
                ],
                "selector": {
                    "deploymentConfig": "session-replication"
                }
            }
        },
        {
            "apiVersion": "v1",
            "id": "session-replication-http-route",
            "kind": "Route",
            "metadata": {
                "annotations": {
                    "description": "Route for application's http service."
                },
                "labels": {
                    "application": "session-replication",
                    "template": "eap6-basic-sti"
                },
                "name": "session-replication-http-route"
            },
            "spec": {
                "host": "session-replication.devops.ose.jbrideratwater.com",
                "to": {
                    "name": "session-replication"
                }
            }
        },
        {
            "apiVersion": "v1",
            "kind": "ImageStream",
            "metadata": {
                "labels": {
                    "application": "session-replication",
                    "template": "eap6-basic-sti"
                },
                "name": "session-replication"
            }
        },
        {
            "apiVersion": "v1",
            "kind": "BuildConfig",
            "metadata": {
                "labels": {
                    "application": "session-replication",
                    "template": "eap6-basic-sti"
                },
                "name": "session-replication"
            },
            "spec": {
                "output": {
                    "to": {
                        "kind": "ImageStreamTag",
                        "name": "session-replication:latest"
                    }
                },
                "source": {
                    "contextDir": "",
                    "git": {
                        "ref": "master",
                        "uri": "https://github.com/gpe-mw-training/session-replication"
                    },
                    "type": "Git"
                },
                "strategy": {
                    "sourceStrategy": {
                        "from": {
                            "kind": "ImageStreamTag",
                            "name": "jboss-eap64-openshift:latest",
                            "namespace": "openshift"
                        },
                    "env": [
                      {
                        "name": "MAVEN_ARGS",
                        "value": "clean package -DskipTests -DOSE_NEXUS_HOST=nexus.admin-project.ose.jbrideratwater.com"
                      }
                    ]
                    },
                    "type": "Source"
                },
                "triggers": [
                    {
                        "github": {
                            "secret": "RiaxUMKv"
                        },
                        "type": "GitHub"
                    },
                    {
                        "generic": {
                            "secret": "rBRvI8v6"
                        },
                        "type": "Generic"
                    },
                    {
                        "imageChange": {},
                        "type": "ImageChange"
                    }
                ]
            }
        },
        {
            "apiVersion": "v1",
            "kind": "BuildConfig",
            "metadata": {
                "labels": {
                    "application": "session-replication",
                    "template": "eap6-basic-sti"
                },
                "name": "session-replication-jenkins"
            },
            "spec": {
                "output": {
                    "to": {
                        "kind": "ImageStreamTag",
                        "name": "session-replication:latest"
                    }
                },
                "source": {
                    "contextDir": "",
                    "git": {
                        "ref": "master",
                        "uri": "https://github.com/gpe-mw-training/session-replication"
                    },
                    "type": "Git"
                },
                "strategy": {
                    "sourceStrategy": {
                        "from": {
                            "kind": "ImageStreamTag",
                            "name": "jboss-eap64-openshift:latest",
                            "namespace": "openshift"
                        },
                 "env": [
                       {
                         "name": "DISABLE_ASSET_COMPILATION",
                         "value": "true"
                       }, {
                         "name": "APPLICATION_ARTIFACT_URL",
                         "value": "http://jenkins.devops.ose.jbrideratwater.com/job/session-replication/lastSuccessfulBuild/artifact/target/session-replication.war"
                       }, {
                         "name": "DOWNLOAD_USER_CREDENTIALS",
                         "value": "admin:password"
                       }
                     ]
                    },
                 "type": "Source"
                },
                "triggers": [
                    {
                        "github": {
                            "secret": "RiaxUMKv"
                        },
                        "type": "GitHub"
                    },
                    {
                        "generic": {
                            "secret": "rBRvI8v6"
                        },
                        "type": "Generic"
                    },
                    {
                        "imageChange": {},
                        "type": "ImageChange"
                    }
                ]
            }
        },
        {
            "apiVersion": "v1",
            "kind": "DeploymentConfig",
            "metadata": {
                "labels": {
                    "application": "session-replication",
                    "template": "eap6-basic-sti"
                },
                "name": "session-replication"
            },
            "spec": {
                "replicas": 0,
                "selector": {
                    "deploymentConfig": "session-replication"
                },
                "strategy": {
                    "type": "Recreate"
                },
                "template": {
                    "metadata": {
                        "labels": {
                            "application": "session-replication",
                            "deploymentConfig": "session-replication"
                        },
                        "name": "session-replication"
                    },
                    "spec": {
                        "containers": [
                            {
                                "env": [
                                    {
                                        "name": "OPENSHIFT_DNS_PING_SERVICE_NAME",
                                        "value": "session-replication-ping"
                                    },
                                    {
                                        "name": "OPENSHIFT_DNS_PING_SERVICE_PORT",
                                        "value": "8888"
                                    },
                                    {
                                        "name": "HORNETQ_CLUSTER_PASSWORD",
                                        "value": "SuMnXIWS"
                                    },
                                    {
                                        "name": "HORNETQ_QUEUES",
                                        "value": ""
                                    },
                                    {
                                        "name": "HORNETQ_TOPICS",
                                        "value": ""
                                    }
                                ],
                                "image": "session-replication",
                                "imagePullPolicy": "Always",
                                "name": "session-replication",
                                "ports": [
                                    {
                                        "containerPort": 8080,
                                        "name": "http",
                                        "protocol": "TCP"
                                    },
                                    {
                                        "containerPort": 8888,
                                        "name": "ping",
                                        "protocol": "TCP"
                                    }
                                ],
                                "readinessProbe": {
                                    "exec": {
                                        "command": [
                                            "/bin/bash",
                                            "-c",
                                            "/opt/eap/bin/readinessProbe.sh"
                                        ]
                                    }
                                }
                            }
                        ]
                    }
                },
                "triggers": [
                    {
                        "imageChangeParams": {
                            "automatic": true,
                            "containerNames": [
                                "session-replication"
                            ],
                            "from": {
                                "kind": "ImageStream",
                                "name": "session-replication"
                            }
                        },
                        "type": "ImageChange"
                    }
                ]
            }
        }
    ]
}
